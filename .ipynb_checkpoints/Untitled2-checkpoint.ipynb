    "a = input(\"Enter a number: \")\n",
    "count = 0\n",
    "if a>2:\n",
    "    for i in range(2,a):\n",
    "        if a%i==0:\n",
    "            count = count+1\n",
    "            break\n",
    "    if count==0:\n",
    "        print \"a is prime number\"\n",
    "    else:\n",
    "        print \"a is not a prime\"\n",
    "else:\n",
    "    print \"not a valid number\""
